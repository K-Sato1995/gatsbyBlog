{
  "name": "gatsby-starter-morning-dew",
  "version": "0.0.0-development",
  "description": "Starter Morning Dew Blog",
  "keywords": [
    "blog",
    "gatsby",
    "gatsby-plugin",
    "gatsby-theme",
    "starter"
  ],
  "homepage": "https://github.com/maxpou/gatsby-starter-morning-dew#readme",
  "bugs": {
    "url": "https://github.com/maxpou/gatsby-starter-morning-dew/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maxpou/gatsby-starter-morning-dew.git"
  },
  "license": "MIT",
  "author": "Maxence Poutord <maxence.poutord@gmail.com>",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js' 'data/siteConfig.js' 'gatsby-*.js' 'scripts/*.js'",
    "generatePostPreviewImages": "./scripts/generatePostPreviewImages.js",
    "lh": "lighthousebot",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "semantic-release": "semantic-release",
    "start": "gatsby develop",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended",
      "plugin:react/recommended",
      "prettier",
      "prettier/react"
    ],
    "rules": {
      "react/prop-types": "off",
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "@svgr/cli": "^5.4.0",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-plugin-styled-components": "^1.10.6",
    "disqus-react": "^1.0.7",
    "gatsby": "^2.15.36",
    "gatsby-image": "^2.2.27",
    "gatsby-plugin-compile-es6-packages": "^2.1.0",
    "gatsby-plugin-google-analytics": "^2.1.21",
    "gatsby-plugin-manifest": "^2.2.21",
    "gatsby-plugin-mdx": "^1.0.51",
    "gatsby-plugin-offline": "^3.0.14",
    "gatsby-plugin-page-creator": "^2.1.25",
    "gatsby-plugin-react-helmet": "^3.1.11",
    "gatsby-plugin-sharp": "^2.2.29",
    "gatsby-plugin-styled-components": "^3.1.17",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-remark-autolink-headers": "^2.1.14",
    "gatsby-remark-copy-linked-files": "^2.1.26",
    "gatsby-remark-embedder": "^2.0.0",
    "gatsby-remark-images": "^3.1.26",
    "gatsby-remark-prismjs": "^3.3.30",
    "gatsby-remark-responsive-iframe": "^2.2.23",
    "gatsby-remark-smartypants": "^2.1.12",
    "gatsby-source-filesystem": "^2.1.31",
    "gatsby-transformer-remark": "^2.6.28",
    "gatsby-transformer-sharp": "^2.2.21",
    "lodash": "^4.17.15",
    "prismjs": "^1.19.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.11.0",
    "styled-components": "^5.0.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.14.2",
    "lighthousebot": "https://github.com/GoogleChromeLabs/lighthousebot",
    "prettier": "^1.18.2",
    "puppeteer": "^1.17.0",
    "semantic-release": "^15.13.18",
    "yaml": "^1.6.0"
  },
  "peerDependencies": {
    "gatsby": "^2.15.0",
    "react": "^16.10.0",
    "react-dom": "^16.10.0"
  }
}
