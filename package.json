{
  "name": "k-sato-blog",
  "version": "v2.0.0",
  "description": "K-Sato's Blog",
  "keywords": [
    "blog",
    "gatsby",
    "gatsby-plugin",
    "gatsby-theme",
    "starter"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js' 'data/siteConfig.js' 'gatsby-*.js' 'scripts/*.js'",
    "generatePostPreviewImages": "./scripts/generatePostPreviewImages.js",
    "lh": "lighthousebot",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "semantic-release": "semantic-release",
    "start": "gatsby develop",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended",
      "plugin:react/recommended",
      "prettier",
      "prettier/react"
    ],
    "rules": {
      "react/prop-types": "off",
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.16",
    "@mdx-js/react": "^1.6.16",
    "@svgr/cli": "^5.4.0",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-plugin-styled-components": "^1.11.1",
    "disqus-react": "^1.0.10",
    "gatsby": "^2.24.53",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-compile-es6-packages": "^2.1.0",
    "gatsby-plugin-google-analytics": "^2.1.21",
    "gatsby-plugin-manifest": "^2.4.27",
    "gatsby-plugin-mdx": "^1.2.37",
    "gatsby-plugin-offline": "^3.2.26",
    "gatsby-plugin-page-creator": "^2.3.25",
    "gatsby-plugin-react-helmet": "^3.1.11",
    "gatsby-plugin-sharp": "^2.6.31",
    "gatsby-plugin-styled-components": "^3.1.17",
    "gatsby-plugin-typescript": "^2.4.18",
    "gatsby-remark-autolink-headers": "^2.1.14",
    "gatsby-remark-copy-linked-files": "^2.3.13",
    "gatsby-remark-embedder": "^2.0.0",
    "gatsby-remark-images": "^3.3.28",
    "gatsby-remark-prismjs": "^3.5.11",
    "gatsby-remark-responsive-iframe": "^2.2.23",
    "gatsby-remark-smartypants": "^2.1.12",
    "gatsby-source-filesystem": "^2.3.27",
    "gatsby-transformer-remark": "^2.8.32",
    "gatsby-transformer-sharp": "^2.5.14",
    "lodash": "^4.17.20",
    "prismjs": "^1.21.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.11.0",
    "react-toc": "^1.2.3",
    "styled-components": "^5.2.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "lighthousebot": "git+https://github.com/GoogleChromeLabs/lighthousebot.git",
    "prettier": "^1.19.1",
    "puppeteer": "^1.17.0",
    "semantic-release": "^15.13.18",
    "yaml": "^1.6.0"
  },
  "peerDependencies": {
    "gatsby": "^2.15.0",
    "react": "^16.10.0",
    "react-dom": "^16.10.0"
  }
}
